{
	"name": "shimmer-theme",
	"version": "0.1.2",
	"description": "A theme may be comfortable",
	"license": "MIT",
	"engines": {
		"vscode": "^1.85.0"
	},
	"contributes": {
		"commands": [
			{
				"command": "extractorWorkbenchColorsSchemaTypes.run",
				"title": "Extractor Workbench Colors Schema Types"
			},
			{
				"command": "parserColorTheme.run",
				"title": "Generate Color Theme Types (Parser Color Theme)"
			}
		]
	},
	"main": "dist/index.cjs",
	"scripts": {
		"dev": "tsx watch src/index.ts",
		"build": "tsx src/index.ts",
		"dev:extension": "rolldown src/extension/index.ts --watch --file dist/index.cjs --config rolldown.config.js",
		"build:extension": "rolldown src/extension/index.ts --file dist/index.cjs --config rolldown.config.js",
		"biome:format": "biome format --write src",
		"biome:check": "biome check --write src",
		"package": "cd build && vsce package --no-dependencies --out ../"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@types/chroma-js": "^3.1.1",
		"@types/node": "^24.7.0",
		"@types/vscode": "^1.104.0",
		"@vscode/vsce": "^3.6.2",
		"chroma-js": "^3.1.2",
		"globals": "^16.4.0",
		"json-schema-to-typescript-lite": "^15.0.0",
		"rolldown": "1.0.0-beta.41",
		"tsx": "^4.20.6"
	}
}
