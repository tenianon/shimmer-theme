  "tokenColors": [
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#8a8b8a"
      }
    },
    {
      "name": "comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6a6b6a"
      }
